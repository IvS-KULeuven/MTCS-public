	MIN_POSITION: LREAL := 260.0;
	///Maximum position of the axis, in micrometer
	MAX_POSITION: LREAL := 1200.0;
	///Screw pitch, in micrometer
	SCREW_PITCH: LREAL := 1000.0;
	///Motor feedback resolution (pulses per revolution of the motor rotor)
